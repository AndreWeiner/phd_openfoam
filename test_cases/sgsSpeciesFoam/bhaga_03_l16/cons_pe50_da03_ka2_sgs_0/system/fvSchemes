FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         Euler;
}

gradSchemes
{
    default         leastSquares;
}

divSchemes
{
    default         Gauss limitedLinear 1.0;
}

laplacianSchemes
{
    default         Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
    interpolate(A)  limitedLinear phi 1.0;
    interpolate(B)  limitedLinear phi 1.0;
    interpolate(P)  limitedLinear phi 1.0;
    interpolate(S)  limitedLinear phi 1.0;
}

snGradSchemes
{
    default         corrected;
}

wallDist
{
}


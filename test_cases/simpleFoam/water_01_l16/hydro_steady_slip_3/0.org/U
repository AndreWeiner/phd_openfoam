FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 -2.93135279 0);

boundaryField
{
    outerDomain
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    bubble
    {
        type            slip;
    }
    "wedge_.*"
    {
        type            wedge;
    }

}



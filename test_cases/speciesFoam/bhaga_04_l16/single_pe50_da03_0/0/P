FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      P;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -3 0 0 1 0 0];

internalField   uniform 0;

boundaryField
{
    outerDomain
    {
        type            zeroGradient;
    }
    bubble
    {
        type            zeroGradient;
    }
    "wedge_.*"
    {
        type            wedge;
    }
}


